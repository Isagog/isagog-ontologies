###  https://ilmanifesto.it/mema/ontology#Action
:Action rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :has_agent ;
                              owl:someValuesFrom :Agent
                            ] ;
        rdfs:subClassOf :Event .


###  https://ilmanifesto.it/mema/ontology#Agent
:Agent rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Class ;
                             owl:unionOf ( :Automata
                                           :Person
                                           :SocialGroup
                                         )
                           ] ;
       rdfs:subClassOf :Continuant ;
       rdfs:comment "Agentive entity"@en ,
                    "Qualsiasi entit√† che interagisce con il contenuto, sia contribuendovi, sia fruendone"@it ;
       :schema "Property::agent"@en .


###  https://ilmanifesto.it/mema/ontology#Article
:Article rdf:type owl:Class ;
         rdfs:subClassOf :Text ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :authored_by ;
                           owl:someValuesFrom :Author
                         ] ;
         rdfs:comment "Authored text"@en ,
                      "Testo auroriale"@it ;
         :schema "https://schema.org/Article" .


###  https://ilmanifesto.it/mema/ontology#Author
:Author rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :author_of ;
                              owl:someValuesFrom :Article
                            ] ;
        rdfs:subClassOf :Information ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :realized_by ;
                          owl:allValuesFrom :Agent
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :signature ;
                          owl:someValuesFrom xsd:string
                        ] ;
        rdfs:comment "Autorship information, realized (but not necessarily) by some agent"@en .


###  https://ilmanifesto.it/mema/ontology#AuthoredInformation
:AuthoredInformation rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :authored_by ;
                                           owl:someValuesFrom [ rdf:type owl:Class ;
                                                                owl:unionOf ( :Agent
                                                                              :Author
                                                                            )
                                                              ]
                                         ] ;
                     rdfs:subClassOf :Information ;
                     :schema "https://schema.org/CreativeWork" .


###  https://ilmanifesto.it/mema/ontology#Automata
:Automata rdf:type owl:Class ;
          rdfs:subClassOf :Object .


###  https://ilmanifesto.it/mema/ontology#Belief
:Belief rdf:type owl:Class ;
        rdfs:subClassOf :Connotation ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :signified ;
                          owl:allValuesFrom [ rdf:type owl:Class ;
                                              owl:unionOf ( :Factoid
                                                            :Statement
                                                          )
                                            ]
                        ] ;
        rdfs:comment "Connotation of a factoid or statement based on the belief \\ disbelief in its factuality or truth"@en .


###  https://ilmanifesto.it/mema/ontology#Category
:Category rdf:type owl:Class ;
          rdfs:subClassOf :Denotation ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :signified ;
                            owl:allValuesFrom :Information
                          ] ;
          rdfs:comment "Categoria di classificazione di oggetti informativi"@it ;
          rdfs:label "Categoria"@it ,
                     "Category"@en .


###  https://ilmanifesto.it/mema/ontology#Collection
:Collection rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :has_member ;
                                  owl:someValuesFrom owl:Thing
                                ] ;
            rdfs:subClassOf :Region .


###  https://ilmanifesto.it/mema/ontology#Connotation
:Connotation rdf:type owl:Class ;
             rdfs:subClassOf :Sign ;
             rdfs:comment "Sign characterizing some aspect of the signified entity"@en .


###  https://ilmanifesto.it/mema/ontology#Continuant
:Continuant rdf:type owl:Class ;
            rdfs:subClassOf :Entity ;
            owl:disjointWith :Occurrent ;
            :schema "Non-temporal entity"@en .


###  https://ilmanifesto.it/mema/ontology#Contributor
:Contributor rdf:type owl:Class ;
             rdfs:subClassOf :Person .


###  https://ilmanifesto.it/mema/ontology#Denotation
:Denotation rdf:type owl:Class ;
            rdfs:subClassOf :Sign ;
            rdfs:comment "Sign that signifies a class of things"@en ;
            rdfs:seeAlso "https://schema.org/Class"@en .


###  https://ilmanifesto.it/mema/ontology#Entity
:Entity rdf:type owl:Class ;
        rdfs:comment "Entit√† localizzabile nello spazio e / o nel tempo"@it ,
                     "Thing located in space and / or time"@en ;
        rdfs:label "Entity"@en ,
                   "Entit√†"@it .


###  https://ilmanifesto.it/mema/ontology#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :Occurrent ;
       rdfs:comment "Specific occurrence or happening, often significant, marked by a change or impact in time and space"@en .


###  https://ilmanifesto.it/mema/ontology#Factoid
:Factoid rdf:type owl:Class ;
         rdfs:subClassOf :Denotation ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :context ;
                           owl:someValuesFrom :Sign
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :contributing_text ;
                           owl:someValuesFrom :Information
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :supported_by ;
                           owl:someValuesFrom :Sign
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :signified ;
                           owl:allValuesFrom :Occurrent
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :signifier ;
                           owl:allValuesFrom :Statement
                         ] ;
         rdfs:comment "The interpretation of an information entity as the evidence that something happened"@en .


###  https://ilmanifesto.it/mema/ontology#Information
:Information rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :realized_by ;
                               owl:someValuesFrom :Continuant
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :signified_by ;
                               owl:someValuesFrom :Sign
                             ] ;
             rdfs:comment "Non material thing realizable in many physical forms, e.g. a newspaper article (written or spoken), classifiers such as tags or topics, word types (lexemes)"@en ;
             rdfs:seeAlso "Representamen (Peirce)" .


###  https://ilmanifesto.it/mema/ontology#Issue
:Issue rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :has_member ;
                             owl:someValuesFrom :Article
                           ] ;
       rdfs:subClassOf :Information ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :has_picture ;
                         owl:someValuesFrom :Picture
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :title ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:string
                       ] ;
       rdfs:comment "Numero del quotidiano" ;
       :schema "https://schema.org/PublicationIssue" .


###  https://ilmanifesto.it/mema/ontology#Lexeme
:Lexeme rdf:type owl:Class ;
        rdfs:subClassOf :Information ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :has_sense ;
                          owl:someValuesFrom :Sense
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :pos ;
                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                               owl:oneOf [ rdf:type rdf:List ;
                                                           rdf:first " VERB" ;
                                                           rdf:rest [ rdf:type rdf:List ;
                                                                      rdf:first "ADJ" ;
                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                 rdf:first "ADV" ;
                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                            rdf:first "MWE" ;
                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                       rdf:first "NOUN" ;
                                                                                                       rdf:rest rdf:nil
                                                                                                     ]
                                                                                          ]
                                                                               ]
                                                                    ]
                                                         ]
                                             ]
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :base_form ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ;
        rdfs:comment "Unit√† del lessico, anche polirematica (multiword expression)"@it .


###  https://ilmanifesto.it/mema/ontology#LivingBeing
:LivingBeing rdf:type owl:Class ;
