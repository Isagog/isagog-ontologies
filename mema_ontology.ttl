#################################################################
#    Classes
#################################################################

###  https://ilmanifesto.it/mema/ontology#Agent
:Agent rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Class ;
                             owl:unionOf ( :Automata
                                           :Organization
                                           :Person
                                         )
                           ] ;
       rdfs:subClassOf :Continuant ;
       rdfs:comment "Agentive entity"@en ,
                    "Qualsiasi entità che interagisce con il contenuto, sia contribuendovi, sia fruendone"@it ;
       :schema "Property::agent"@en .


###  https://ilmanifesto.it/mema/ontology#Article
:Article rdf:type owl:Class ;
         rdfs:subClassOf :Text ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :authored_by ;
                           owl:someValuesFrom :Author
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :has_part ;
                           owl:someValuesFrom :Title
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :part_of ;
                           owl:someValuesFrom :Issue
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :text ;
                           owl:someValuesFrom xsd:string
                         ] ;
         rdfs:comment "Articolo del quotidiano"@it ;
         :schema "https://schema.org/NewsArticle" .


###  https://ilmanifesto.it/mema/ontology#Author
:Author rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :author_of ;
                              owl:someValuesFrom :Information
                            ] ;
        rdfs:subClassOf :Information ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :realized_by ;
                          owl:allValuesFrom :Agent
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :signature ;
                          owl:someValuesFrom xsd:string
                        ] ;
        :mema_db "" .


###  https://ilmanifesto.it/mema/ontology#AuthoredInformation
:AuthoredInformation rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :authored_by ;
                                           owl:someValuesFrom [ rdf:type owl:Class ;
                                                                owl:unionOf ( :Agent
                                                                              :Author
                                                                            )
                                                              ]
                                         ] ;
                     rdfs:subClassOf :Information ;
                     :schema "https://schema.org/CreativeWork" .


###  https://ilmanifesto.it/mema/ontology#Automata
