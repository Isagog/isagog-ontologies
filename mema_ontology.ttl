                           owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                               owl:oneOf [ rdf:type rdf:List ;
                                                           rdf:first "ACCEPTED" ;
                                                           rdf:rest [ rdf:type rdf:List ;
                                                                      rdf:first "PENDING" ;
                                                                      rdf:rest rdf:nil
                                                                    ]
                                                         ]
                                             ]
                         ] ;
         rdfs:comment "Reference to an Entity in a Text"@en .


