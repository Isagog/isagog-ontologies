###  https://ilmanifesto.it/mema/ontology#pos
:pos rdf:type owl:DatatypeProperty ;
     rdfs:domain :Lexeme ;
     rdfs:range [ rdf:type rdfs:Datatype ;
                  owl:oneOf [ rdf:type rdf:List ;
                              rdf:first " VERB" ;
                              rdf:rest [ rdf:type rdf:List ;
                                         rdf:first "ADJ" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "ADV" ;
                                                    rdf:rest [ rdf:type rdf:List ;
                                                               rdf:first "MWE" ;
                                                               rdf:rest [ rdf:type rdf:List ;
                                                                          rdf:first "NOUN" ;
                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                     rdf:first "PROPN" ;
                                                                                     rdf:rest rdf:nil
                                                                                   ]
                                                                        ]
                                                             ]
                                                  ]
                                       ]
                            ]
                ] ;
     rdfs:comment "part of speech"@en ,
                  "parte del discorso (standard universal dependencies)"@it .


