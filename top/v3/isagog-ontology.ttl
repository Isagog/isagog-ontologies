@prefix : <https://isagog.com/ontology/top#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://isagog.com/ontology/top#> .

<https://isagog.com/ontology/top> rdf:type owl:Ontology ;
                                   owl:versionIRI <https://isagog.com/ontology/top/3.0> ;
                                   <http://purl.org/dc/elements/1.1/creator> "Guido Vetere, Isagog Srl, Italy" ;
                                   <http://purl.org/dc/elements/1.1/license> <http://creativecommons.org/licenses/by/4.0/> ;
                                   rdfs:comment """An ultra-lite top level that provides a minimal set of foundational categories designed to capture the most general distinctions presupposed in our conceptual scheme. The categories employed are ontologically modest: they are introduced only insofar as they are required to make sense of actual, observable practices of reference and predication.
The result is a lightweight framework suited for AI driven human-computer interaction, with a particular sensitivity to the contextual, relational, and interpretive dimensions of meaning."""@en ,
                                                """Un livello superiore ultra-leggero che fornisce un insieme minimo di categorie fondazionali progettate per catturare le distinzioni più generali presupposte nel nostro schema concettuale. Le categorie impiegate sono ontologicamente modeste: sono introdotte solo nella misura in cui sono necessarie per dare senso alle pratiche effettive e osservabili di riferimento e predicazione.
Il risultato è un framework leggero adatto all'interazione uomo-computer guidata dall'IA, con particolare sensibilità alle dimensioni contestuali, relazionali e interpretative del significato."""@it ;
                                   rdfs:label "Isagog Top Level Ontology"@en ,
                                              "Ontologia di Livello Superiore Isagog"@it ;
                                   owl:versionInfo "3.0 29/11/2025" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/license
<http://purl.org/dc/elements/1.1/license> rdf:type owl:AnnotationProperty .


###  https://isagog.com/ontology/top#dolce
:dolce rdf:type owl:AnnotationProperty ;
       rdfs:comment "Mappatura verso l'ontologia DOLCE"@it ,
                    "Mapping to DOLCE ontology"@en ;
       rdfs:label "DOLCE mapping"@en ,
                  "mappatura DOLCE"@it ;
       rdfs:subPropertyOf :mapping .


###  https://isagog.com/ontology/top#example
:example rdf:type owl:AnnotationProperty .


###  https://isagog.com/ontology/top#mapping
:mapping rdf:type owl:AnnotationProperty ;
         rdfs:comment "Annotazione di mappatura generica"@it ,
                      "Generic mapping annotation"@en ;
         rdfs:label "mappatura"@it ,
                    "mapping"@en .


###  https://isagog.com/ontology/top#meta
:meta rdf:type owl:AnnotationProperty ;
      rdfs:comment "Annotazione a livello meta"@it ,
                   "Meta-level annotation"@en ;
      rdfs:label "meta"@en ,
                 "meta"@it .


###  https://isagog.com/ontology/top#schema
:schema rdf:type owl:AnnotationProperty ;
        rdfs:comment "Correspondence with schema.org"@en ,
                     "Corrispondenza con schema.org"@it ;
        rdfs:label "mappatura schema.org"@it ,
                   "schema.org mapping"@en ;
        rdfs:subPropertyOf :mapping .


#################################################################
#    Object Properties
#################################################################

###  https://isagog.com/ontology/top#adjacent_to
:adjacent_to rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :connectedness ;
             rdf:type owl:SymmetricProperty ;
             rdfs:domain :Location ;
             rdfs:range :Location ;
             rdfs:comment "Connessione spaziale"@it ,
                          "Spatial connectedness"@en ;
             rdfs:label "adiacente a"@it ,
                        "adjacent to"@en .


###  https://isagog.com/ontology/top#caused_by
:caused_by rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :entailment ;
           owl:inverseOf :causes ;
           rdfs:domain :Occurrent ;
           rdfs:range :Occurrent .


###  https://isagog.com/ontology/top#causes
:causes rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :entailment ;
        rdfs:domain :Occurrent ;
        rdfs:range :Occurrent .


###  https://isagog.com/ontology/top#collocation
:collocation rdf:type owl:ObjectProperty ;
             rdfs:comment "Collocazione spaziale"@it ,
                          "Spatial collocation"@en ;
             rdfs:label "collocation"@en ,
                        "collocazione"@it .


###  https://isagog.com/ontology/top#composition
:composition rdf:type owl:ObjectProperty ;
             rdfs:comment "General part-whole relationship"@en ,
                          "Relazione generale parte-tutto"@it ;
             rdfs:label "composition"@en ,
                        "composizione"@it .


###  https://isagog.com/ontology/top#connectedness
:connectedness rdf:type owl:ObjectProperty ;
               rdfs:comment "prossimità spazio-temporale"@it ,
                            "spatial temporal proximity"@en ;
               rdfs:label "connectedness"@en ,
                          "connessione"@it .


###  https://isagog.com/ontology/top#element_of
:element_of rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :composition ;
            owl:inverseOf :has_element ;
            rdfs:domain :Continuant ;
            rdfs:range :Continuant ;
            rdfs:comment "Appartenenza"@it ,
                         "Membership"@en ;
            rdfs:label "element of"@en ,
                       "elemento di"@it .


###  https://isagog.com/ontology/top#entailed_by
:entailed_by rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :entailment ;
             owl:inverseOf :entails ;
             rdfs:domain :Concept ;
             rdfs:range :Concept ;
             rdfs:comment "Inverse entailment relation"@en ,
                          "Relazione di implicazione inversa"@it ;
             rdfs:label "entailed by"@en ,
                        "implicato da"@it .


###  https://isagog.com/ontology/top#entailment
:entailment rdf:type owl:ObjectProperty ;
            rdfs:comment "Logical relation in which the truth of one proposition necessitates the truth of another"@en ,
                         "Relazione logica in cui la verità di una proposizione rende necessaria la verità di un'altra"@it ;
            rdfs:label "entailment"@en ,
                       "implicazione"@it .


###  https://isagog.com/ontology/top#entails
:entails rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :entailment ;
         rdfs:domain :Concept ;
         rdfs:range :Concept ;
         rdfs:comment "Direct entailment relation"@en ,
                      "Relazione di implicazione diretta"@it ;
         rdfs:label "entails"@en ,
                    "implica"@it .


###  https://isagog.com/ontology/top#follows
:follows rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :connectedness ;
         owl:inverseOf :precedes ;
         rdf:type owl:AsymmetricProperty ;
         rdfs:domain :Event ;
         rdfs:range :Event ;
         rdfs:comment "Connessione temporale"@it ,
                      "Temporal connectedness"@en ;
         rdfs:label "next to"@en ,
                    "successivo a"@it .


###  https://isagog.com/ontology/top#has_element
:has_element rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :composition ;
             rdfs:domain :Collection ;
             rdfs:range :Continuant ;
             rdfs:comment "Appartenenza"@it ,
                          "Membership"@en ;
             rdfs:label "ha elemento"@it ,
                        "has element"@en .


###  https://isagog.com/ontology/top#has_inherent
:has_inherent rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :inherence ;
              owl:inverseOf :inheres_in ;
              rdfs:range :Quality ;
              rdfs:comment "Relation to inherent intangible aspects"@en ,
                           "Relazione verso aspetti intangibili inerenti"@it ;
              rdfs:label "ha inerente"@it ,
                         "has inherent"@en .


###  https://isagog.com/ontology/top#has_part
:has_part rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :composition ;
          owl:inverseOf :part_of ;
          rdfs:domain :Continuant ;
          rdfs:range :Continuant ;
          rdfs:comment "Material parthood (whole - part)"@en ,
                       "Partizione materiale (tutto - parte)"@it ;
          rdfs:label "ha parte"@it ,
                     "has part"@en .


###  https://isagog.com/ontology/top#has_participant
:has_participant rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :participation ;
                 owl:inverseOf :participate_in ;
                 rdfs:domain :Occurrent ;
                 rdfs:range :Continuant ;
                 rdfs:comment "Participation relation from event to participant"@en ,
                              "Relazione di partecipazione dall'evento al partecipante"@it ;
                 rdfs:label "ha partecipante"@it ,
                            "has participant"@en .


###  https://isagog.com/ontology/top#has_phase
:has_phase rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :composition ;
           owl:inverseOf :phase_of ;
           rdfs:domain :Occurrent ;
           rdfs:range :Occurrent ;
           rdfs:comment "Partizione temporale (tutto - parte)"@it ,
                        "Temporal parthood (whole - part)"@en ;
           rdfs:label "ha fase"@it ,
                      "has phase"@en .


###  https://isagog.com/ontology/top#inherence
:inherence rdf:type owl:ObjectProperty ;
           rdfs:comment "Relation of information objects, their subjects and substrate"@en ,
                        "Relazione degli oggetti informativi, i loro soggetti e substrato"@it ;
           rdfs:label "inerenza"@it ,
                      "inherence"@en .


###  https://isagog.com/ontology/top#inheres_in
:inheres_in rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :inherence ;
            rdfs:domain :Quality ;
            rdfs:comment "Any relation involved in semiotic processes"@en ,
                         "Qualsiasi relazione coinvolta in processi semiotici"@it ;
            rdfs:label "inerisce in"@it ,
                       "inheres in"@en .


###  https://isagog.com/ontology/top#interpretant
:interpretant rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :has_part ;
              rdfs:domain :Sign ;
              rdfs:range :Concept ;
              rdfs:comment "collega un Segno a un Concetto come suo Interpretante, cioè l'effetto o comprensione che il segno produce in una mente, secondo la triade semiotica di Peirce."@it ,
                           "links a Sign to a Concept as its Interpretant, i.e., the effect or understanding the sign produces in a mind, according to Peirce's semiotic triad."@en ;
              rdfs:label "interpretant"@en ,
                         "interpretante"@it .


###  https://isagog.com/ontology/top#is_location_of
:is_location_of rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :collocation ;
                owl:inverseOf :located_in ;
                rdfs:domain :Location ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :Occurrent
                                           :Tangible
                                         )
                           ] ;
                rdfs:comment "Location relation from place to entity or event"@en ,
                             "Relazione di ubicazione dal luogo all'entità o all'evento"@it ;
                rdfs:label "is location of"@en ,
                           "è ubicazione di"@it .


###  https://isagog.com/ontology/top#located_in
:located_in rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :collocation ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Continuant
                                        :Tangible
                                      )
                        ] ;
            rdfs:range :Location ;
            rdfs:comment "Location relation from entity to place"@en ,
                         "Relazione di ubicazione dall'entità al luogo"@it ;
            rdfs:label "located in"@en ,
                       "ubicato in"@it .


###  https://isagog.com/ontology/top#part_of
:part_of rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :composition ;
         rdfs:comment "Material parthood (part-whole)"@en ,
                      "Partizione materiale (parte-tutto)"@it ;
         rdfs:label "part of"@en ,
                    "parte di"@it .


###  https://isagog.com/ontology/top#participate_in
:participate_in rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :participation ;
                rdfs:domain :Continuant ;
                rdfs:range :Occurrent ;
                rdfs:comment "Partecipazione del continuante nell'occorrente"@it ,
                             "Participation of continuant in occurrent"@en ;
                rdfs:label "partecipa in"@it ,
                           "participates in"@en .


###  https://isagog.com/ontology/top#participation
:participation rdf:type owl:ObjectProperty ;
               rdfs:comment "Continuant participation in occurrents"@en ,
                            "Partecipazione dei continuanti negli occorrenti"@it ;
               rdfs:label "partecipazione"@it ,
                          "participation"@en .


###  https://isagog.com/ontology/top#phase_of
:phase_of rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :composition ;
          rdfs:domain :Occurrent ;
          rdfs:range :Occurrent ;
          rdfs:comment "Partizione temporale (parte - tutto)"@it ,
                       "Temporal parthood (part - whole)"@en ;
          rdfs:label "fase di"@it ,
                     "phase of"@en .


###  https://isagog.com/ontology/top#precedes
:precedes rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :connectedness .


###  https://isagog.com/ontology/top#referent
:referent rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :has_part ;
          owl:inverseOf :referred_by ;
          rdfs:domain :Sign ;
          rdfs:comment "'Real-world' concept or entity that the sign points to or refers to."@en ,
                       "Concetto o entità del 'mondo reale' a cui il segno punta o si riferisce."@it ;
          rdfs:label "referent"@en ,
                     "referente"@it ;
          rdfs:seeAlso "Sign->Object (Peirce)" .


###  https://isagog.com/ontology/top#referred_by
:referred_by rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :part_of ;
             rdfs:comment "Being referred by a sign"@en ,
                          "Essere riferito da un segno"@it ;
             rdfs:label "referred by"@en ,
                        "riferito da"@it .


###  https://isagog.com/ontology/top#source
:source rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :inherence ;
        owl:inverseOf :yields ;
        rdfs:domain :Sign ;
        rdfs:range :Information ;
        rdfs:comment "Sorgente che produce il segno"@it ,
                     "Source that yields the sign"@en ;
        rdfs:label "sorgente"@it ,
                   "source"@en .


###  https://isagog.com/ontology/top#yields
:yields rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :inherence ;
        rdfs:domain :Information ;
        rdfs:range :Sign ;
        rdfs:comment "Production of signs from intangibles"@en ,
                     "Produzione di segni da intangibili"@it ;
        rdfs:label "produce"@it ,
                   "yields"@en .


#################################################################
#    Data properties
#################################################################

###  https://isagog.com/ontology/top#anthroponym
:anthroponym rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :identifier ;
             rdfs:range xsd:string ;
             rdfs:comment "Identificatore del nome personale"@it ,
                          "Personal name identifier"@en ;
             rdfs:label "anthroponym"@en ,
                        "antroponimo"@it .


###  https://isagog.com/ontology/top#concept
:concept rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :identifier ;
         rdfs:domain :Sign ;
         rdfs:comment "mappatura dei Segni verso predicati interpretanti (concetti o proprietà)"@it ,
                      "mapping of Signs to interpreting predicates (concepts or properties)"@en ;
         rdfs:label "concept"@en ,
                    "concetto"@it ;
         rdfs:seeAlso "Representamen->Interpretant (Peirce)" .


###  https://isagog.com/ontology/top#coordinate
:coordinate rdf:type owl:DatatypeProperty ;
            rdfs:comment "Generic coordinate value"@en ,
                         "Valore generico di coordinata"@it ;
            rdfs:label "coordinata"@it ,
                       "coordinate"@en .


###  https://isagog.com/ontology/top#dimension
:dimension rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain :Sort ;
           rdfs:comment "Dimensional property of a sortal"@en ,
                        "Proprietà dimensionale di un sortale"@it ;
           rdfs:label "dimension"@en ,
                      "dimensione"@it .


###  https://isagog.com/ontology/top#geo_coordinate
:geo_coordinate rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :coordinate ;
                rdfs:comment "any suitable <longitude, latitude> coordinate"@en ,
                             "qualsiasi coordinata <longitudine, latitudine> adatta"@it ;
                rdfs:label "coordinata geografica"@it ,
                           "geo coordinate"@en .


###  https://isagog.com/ontology/top#identifier
:identifier rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:string ;
            rdfs:comment "Generic identifier string"@en ,
                         "Stringa identificativa generica"@it ;
            rdfs:label "identificatore"@it ,
                       "identifier"@en .


###  https://isagog.com/ontology/top#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf :identifier ;
      rdfs:range xsd:string ;
      rdfs:comment "Identificatore del nome"@it ,
                   "Name identifier"@en ;
      rdfs:label "name"@en ,
                 "nome"@it .


###  https://isagog.com/ontology/top#signifier
:signifier rdf:type owl:DatatypeProperty ;
           rdfs:domain :Sign ;
           rdfs:comment "Rappresentamen o forma materiale del segno"@it ,
                        "Sign's representamen or material form"@en ;
           rdfs:label "significante"@it ,
                      "signifier"@en .


###  https://isagog.com/ontology/top#surname
:surname rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :identifier ;
         rdfs:range xsd:string ;
         rdfs:comment "Family name identifier"@en ,
                      "Identificatore del cognome"@it ;
         rdfs:label "cognome"@it ,
                    "surname"@en .


###  https://isagog.com/ontology/top#text
:text rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdfs:range xsd:string ;
      rdfs:comment "Significante testuale"@it ,
                   "Textual significant"@en ;
      rdfs:label "testo"@it ,
                 "text"@en .


###  https://isagog.com/ontology/top#time_region
:time_region rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :coordinate ;
             rdfs:comment "any suitable time point or interval"@en ,
                          "qualsiasi punto o intervallo temporale adatto"@it ;
             rdfs:label "coordinata temporale"@it ,
                        "time coordinate"@en .


#################################################################
#    Classes
#################################################################

###  https://isagog.com/ontology/top#Animal
:Animal rdf:type owl:Class ;
        rdfs:subClassOf :Sort .


###  https://isagog.com/ontology/top#Artifact
:Artifact rdf:type owl:Class ;
          rdfs:subClassOf :Sort ;
          rdfs:label "Artifact"@en ,
                     "Artefatto" .


###  https://isagog.com/ontology/top#Collection
:Collection rdf:type owl:Class ;
            rdfs:subClassOf :Intangible ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :has_element ;
                              owl:someValuesFrom owl:Thing
                            ] .


###  https://isagog.com/ontology/top#Concept
:Concept rdf:type owl:Class ;
         rdfs:subClassOf :Intangible ;
         rdfs:comment "Oggetto sociale che identifica un concetto. Diversamente dalle qualità, i concetti non dipendono esistenzialmente dalle entità."@it ,
                      "Social object that identify a concept. Differently from qualities, concepts do not existentially depend on entities."@en ;
         rdfs:label "Concept"@en ,
                    "Concetto"@it .


###  https://isagog.com/ontology/top#Continuant
:Continuant rdf:type owl:Class ;
            rdfs:comment "Cosa materiale che persiste nel tempo mantenendo la propria identità, esistendo come un tutto in ogni momento dato. I continuanti non sono caratterizzati da parti temporali; invece, perdurano come la stessa entità attraverso il cambiamento, distinti da processi o eventi che si dispiegano nel tempo."@it ,
                         "Material thing that persists through time while maintaining its identity, existing as a whole at any given moment. Continuants are not characterized by temporal parts; instead, they endure as the same entity throughout change, distinct from processes or events, which unfold over time."@en ;
            rdfs:label "Continuant"@en ,
                       "Continuante"@it ;
            :dolce "http://www.loa-cnr.it/ontologies/DUL.owl#Object"@en .


###  https://isagog.com/ontology/top#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :Occurrent ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :has_participant ;
                         owl:someValuesFrom :Continuant
                       ] ;
       rdfs:comment "A temporally extended occurrence composed of heterogeneous parts or phases."@en ,
                    "Un'occorrenza temporalmente estesa composta da parti o fasi eterogenee."@it ;
       rdfs:label "Event"@en ,
                  "Evento"@it .


###  https://isagog.com/ontology/top#Information
:Information rdf:type owl:Class ;
             rdfs:subClassOf :Intangible ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :yields ;
                               owl:someValuesFrom :Sign
                             ] ;
             rdfs:comment "Intangible objects that yields signs, which may be instantiated in many physical forms, e.g. documents, descriptors, classifiers such as tags or topics, word types (lexemes) and tokens."@en ,
                          "Oggetti intangibili che producono segni, che possono essere istanziati in molte forme fisiche, es. documenti, descrittori, classificatori come tag o argomenti, tipi di parole (lessemi) e token."@it ;
             rdfs:label "Information"@en ,
                        "Informazione"@it ;
             :dolce "http://www.loa-cnr.it/ontologies/DUL.owl#InformationEntity"@en .


###  https://isagog.com/ontology/top#Intangible
:Intangible rdf:type owl:Class ;
            rdfs:subClassOf :Continuant ;
            rdfs:comment "Entità non materiale (senza attributi spazio-temporali diretti), che è socialmente riconosciuta, come: oggetti informativi (es. documenti), idee, teorie, qualità"@it ,
                         "Non material thing (without direct temporal-spatial attributes), which is socially recognized, such as: informative objects (e.g. documents), ideas, theories, qualities"@en ;
            rdfs:label "Intangibile"@it ,
                       "Intangible"@en .


###  https://isagog.com/ontology/top#Location
:Location rdf:type owl:Class ;
          rdfs:subClassOf :Continuant ;
          rdfs:comment "Identified region of space"@en ,
                       "Regione identificata di spazio"@it ;
          rdfs:label "Location"@en ,
                     "Luogo"@it .


###  https://isagog.com/ontology/top#NaturalThing
:NaturalThing rdf:type owl:Class ;
              rdfs:subClassOf :Sort ;
              rdfs:comment "Natural object in the vegetal or natural realm"@en ,
                           "Oggetto naturale nel regno vegetale o minerale"@it ;
              rdfs:label "Natural object"@en ,
                         "Oggetto naturale"@it .


###  https://isagog.com/ontology/top#Occurrent
:Occurrent rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :time_region ;
                             owl:someValuesFrom xsd:string
                           ] ;
           rdfs:comment "Cosa che si dispiega o occorre nel tempo, come eventi, stati di cose, processi o attività. Queste entità non sono completamente presenti in un singolo momento ma sono estese nel tempo, in contrasto con i 'Continuanti', che persistono mantenendo l'identità. Ogni evento o stato di cose avviene in un luogo e riguarda qualcosa o qualcuno."@it ,
                        "Thing that unfold or occur over time, such as events, states, processes, or activities. These entities are not wholly present at any single moment but are extended in time, contrasting with 'Continuants', which persist while maintaining identity. Every event or state occurs in a place and involves something or someone."@en ;
           rdfs:label "Occorrente"@it ,
                      "Occurrent"@en ;
           :dolce "http://www.loa-cnr.it/ontologies/DUL.owl#Event"@en .


###  https://isagog.com/ontology/top#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf :Collection ;
