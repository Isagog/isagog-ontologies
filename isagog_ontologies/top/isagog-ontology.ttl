###  https://isagog.com/ontology/top#located_in
:located_in rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :collocation ;
            rdfs:domain :Continuant ;
            rdfs:range :Place ;
            rdfs:comment "Location relation from entity to place"@en ,
                         "Relazione di ubicazione dall'entità al luogo"@it ;
            rdfs:label "located in"@en ,
                       "ubicato in"@it .


###  https://isagog.com/ontology/top#logical_part_of
:logical_part_of rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :part_of ;
                 rdfs:domain :Intangible ;
                 rdfs:range :Intangible ;
                 rdfs:comment "Logical parthood relation between intangibles"@en ,
                              "Relazione di partizione logica tra intangibili"@it ;
