        rdfs:comment "Sorgente che produce il segno"@it ,
                     "Source that yields the sign"@en ;
        rdfs:label "sorgente"@it ,
                   "source"@en .


###  https://isagog.com/ontology/top#spatial_part_of
:spatial_part_of rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :part_of ;
                 rdfs:domain :Sortal ;
                 rdfs:range :Sortal ;
                 rdfs:comment "Relazione di partizione spaziale"@it ,
                              "Spatial parthood relation"@en ;
                 rdfs:label "parte spaziale di"@it ,
                            "spatial part of"@en .


###  https://isagog.com/ontology/top#subject
:subject rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :inheres_in ;
         rdfs:domain :Relationship ;
         rdfs:comment "Soggetto di una relazione"@it ,
                      "Subject of a relationship"@en ;
         rdfs:label "soggetto"@it ,
                    "subject"@en .


###  https://isagog.com/ontology/top#yields
:yields rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :has_inherent ;
        rdfs:domain :Intangible ;
        rdfs:range :Sign ;
        rdfs:comment "Production of signs from intangibles"@en ,
                     "Produzione di segni da intangibili"@it ;
        rdfs:label "produce"@it ,
                   "yields"@en .


