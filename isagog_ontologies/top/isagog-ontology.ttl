           rdfs:comment "Partizione temporale (tutto - parte)"@it ,
                        "Temporal parthood (whole - part)"@en ;
           rdfs:label "ha fase"@it ,
                      "has phase"@en .


###  https://isagog.com/ontology/top#has_quality
:has_quality rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :has_inherent ;
