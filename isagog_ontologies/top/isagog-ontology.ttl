             owl:inverseOf :has_category ;
             rdfs:comment "Relation from category to categorized entity"@en ,
                          "Relazione dalla categoria all'entità categorizzata"@it ;
             rdfs:label "categoria di"@it ,
                        "category of"@en .


###  https://isagog.com/ontology/top#classified_by
:classified_by rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :has_inherent ;
               owl:inverseOf :classifies ;
               rdfs:comment "Inverse of classification relation"@en ,
                            "Inverso della relazione di classificazione"@it ;
               rdfs:label "classificato da"@it ,
                          "classified by"@en .


###  https://isagog.com/ontology/top#classifies
:classifies rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :inheres_in ;
            rdfs:comment "Classification relation from classifier to classified"@en ,
                         "Relazione di classificazione dal classificatore al classificato"@it ;
            rdfs:label "classifica"@it ,
                       "classifies"@en .


###  https://isagog.com/ontology/top#collocation
:collocation rdf:type owl:ObjectProperty ;
             rdfs:comment "Collocazione spaziale"@it ,
                          "Spatial collocation"@en ;
             rdfs:label "collocation"@en ,
                        "collocazione"@it .


###  https://isagog.com/ontology/top#composition
:composition rdf:type owl:ObjectProperty ;
             rdfs:comment "General part-whole relationship"@en ,
                          "Relazione generale parte-tutto"@it ;
