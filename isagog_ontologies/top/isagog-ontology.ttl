             rdfs:label "adiacente a"@it ,
                        "adjacent to"@en .


###  https://isagog.com/ontology/top#afford_as
:afford_as rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :inheres_in ;
           rdfs:domain :Affordance ;
           rdfs:range :Occurrent ;
           rdfs:comment "affordance possible outcome"@en ,
                        "esito possibile dell'affordance"@it ;
           rdfs:label "affords as"@en ,
                      "permette come"@it ;
           :modality "possibility" .


###  https://isagog.com/ontology/top#afford_from
:afford_from rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :inheres_in ;
             rdfs:domain :Affordance ;
             rdfs:range :Continuant ;
             rdfs:comment "affordance entity grounding"@en ,
                          "fondamento entitativo dell'affordance"@it ;
             rdfs:label "affords from"@en ,
                        "permette da"@it ;
             :modality "possibility" .


###  https://isagog.com/ontology/top#afford_in
:afford_in rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :inheres_in ;
           rdfs:domain :Affordance ;
           rdfs:range :Occurrent ;
           rdfs:comment "affordance possible context"@en ,
                        "contesto possibile dell'affordance"@it ;
           rdfs:label "affords in"@en ,
                      "permette in"@it ;
           :modality "possibility" .


###  https://isagog.com/ontology/top#afford_to
:afford_to rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :inheres_in ;
           rdfs:domain :Affordance ;
           rdfs:range :Agent ;
           rdfs:comment "affordance possible subject (to whom the affordance is)"@en ,
                        "soggetto possibile dell'affordance (a chi l'affordance si rivolge)"@it ;
           rdfs:label "affords to"@en ,
                      "permette a"@it ;
           :modality "possibility" .


###  https://isagog.com/ontology/top#author_of
:author_of rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :relatedness ;
           owl:inverseOf :authored_by ;
           rdfs:comment "Authorship relation from author to work"@en ,
                        "Relazione di autorialità dall'autore all'opera"@it ;
           rdfs:label "author of"@en ,
                      "autore di"@it .


###  https://isagog.com/ontology/top#authored_by
:authored_by rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :relatedness ;
             rdfs:comment "Authorship relation from work to author"@en ,
                          "Relazione di autorialità dall'opera all'autore"@it ;
             rdfs:label "authored by"@en ,
                        "scritto da"@it .


###  https://isagog.com/ontology/top#category_of
:category_of rdf:type owl:ObjectProperty ;
