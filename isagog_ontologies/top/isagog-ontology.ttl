#################################################################
#    Classes
#################################################################

###  https://isagog.com/ontology/top#Action
:Action rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :initiated_by ;
                              owl:someValuesFrom :Agent
                            ] ;
        rdfs:subClassOf :Event ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :planned_by ;
                          owl:someValuesFrom :Information
                        ] ;
        rdfs:label "Action"@en ,
                  "Azione"@it ;
        rdfs:comment "Event initiated by an agent with intentional planning"@en ,
                    "Evento iniziato da un agente con pianificazione intenzionale"@it .


###  https://isagog.com/ontology/top#Affordance
:Affordance rdf:type owl:Class ;
            rdfs:subClassOf :Disposition ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :qualifies ;
                              owl:allValuesFrom :Continuant
                            ] ;
            rdfs:label "Affordance"@en ,
                      "Affordance"@it ;
            rdfs:comment "Disposition of Continuants to be acted upon by agents with respect to practical possibilities."@en ,
                        "Disposizione dei Continuanti ad essere agiti da agenti rispetto a possibilità pratiche."@it ;
            :modality "possibility" .


###  https://isagog.com/ontology/top#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf :Sortal ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :initiates ;
                         owl:someValuesFrom :Event
                       ] ;
       rdfs:label "Agent"@en ,
                 "Agente"@it ;
       rdfs:comment "A sortal capable of initiating events"@en ,
                   "Un sortale capace di iniziare eventi"@it .


###  https://isagog.com/ontology/top#Automata
:Automata rdf:type owl:Class ;
          rdfs:subClassOf :Agent ;
          rdfs:label "Automata"@en ,
                    "Automa"@it ;
          rdfs:comment "Computational or mechanical systems capable of executing predefined sequences of operations or behaviors"@en ,
                      "Sistemi computazionali o meccanici capaci di eseguire sequenze predefinite di operazioni o comportamenti"@it .


###  https://isagog.com/ontology/top#Concept
:Concept rdf:type owl:Class ;
         rdfs:subClassOf :Intangible ;
         rdfs:label "Concept"@en ,
                   "Concetto"@it ;
         rdfs:comment "Intangible object that exists through collective intentionality and shared recognition through linguistic acts (see Searle, social object). Differently from qualities, concepts do not existentially depend on entities."@en ,
                     "Oggetto intangibile che esiste attraverso l'intenzionalità collettiva e il riconoscimento condiviso attraverso atti linguistici (vedi Searle, oggetto sociale). Diversamente dalle qualità, i concetti non dipendono esistenzialmente dalle entità."@it .


###  https://isagog.com/ontology/top#Continuant
:Continuant rdf:type owl:Class ;
            owl:disjointWith :Intangible ,
                             :Occurrent ;
            rdfs:label "Continuant"@en ,
                      "Continuante"@it ;
            rdfs:comment "Material thing that persists through time while maintaining its identity, existing as a whole at any given moment. Continuants are not characterized by temporal parts; instead, they endure as the same entity throughout change, distinct from processes or events, which unfold over time."@en ,
                        "Cosa materiale che persiste nel tempo mantenendo la propria identità, esistendo come un tutto in ogni momento dato. I continuanti non sono caratterizzati da parti temporali; invece, perdurano come la stessa entità attraverso il cambiamento, distinti da processi o eventi che si dispiegano nel tempo."@it ;
            :dolce "http://www.loa-cnr.it/ontologies/DUL.owl#Object"@en .


###  https://isagog.com/ontology/top#Description
:Description rdf:type owl:Class ;
             rdfs:subClassOf :Statement ;
             rdfs:label "Description"@en ,
                       "Descrizione"@it ;
             rdfs:comment "Statement that brings information about a specific aspect or feature of something"@en ,
                         "Enunciato che porta informazione su un aspetto o caratteristica specifica di qualcosa"@it .


###  https://isagog.com/ontology/top#Disposition
:Disposition rdf:type owl:Class ;
             rdfs:subClassOf :Quality ;
             rdfs:label "Disposition"@en ,
                       "Disposizione"@it ;
             rdfs:comment "Potential or modal quality that inheres in a bearer but requires specific contextual conditions for its full realization or manifestation. It captures the capacity, tendency, or functional potential of an entity."@en ,
                         "Qualità potenziale o modale che inerisce in un portatore ma richiede condizioni contestuali specifiche per la sua piena realizzazione o manifestazione. Cattura la capacità, tendenza o potenziale funzionale di un'entità."@it .


###  https://isagog.com/ontology/top#Document
:Document rdf:type owl:Class ;
          rdfs:subClassOf :Information ,
                          _:genid5 ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :body ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ;
          rdfs:label "Document"@en ,
                    "Documento"@it ;
          rdfs:comment "Textual document"@en ,
                      "Documento testuale"@it .

_:genid5 rdf:type owl:Restriction ;
          owl:onProperty :authored_by ;
          owl:someValuesFrom :Person .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Document ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget _:genid5 ;
   <https://ilmanifesto.it/mema/ontology#we_filter> "true"^^xsd:boolean ;
   <https://ilmanifesto.it/mema/ontology#we_search> "true"^^xsd:boolean ;
   <https://ilmanifesto.it/mema/ontology#we_tok> "FIELD"
 ] .


###  https://isagog.com/ontology/top#Enactment
:Enactment rdf:type owl:Class ;
           rdfs:subClassOf :Disposition ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :qualifies ;
                             owl:allValuesFrom :Occurrent
                           ] ;
           rdfs:label "Enactment"@en ,
                     "Attuazione"@it ;
           rdfs:comment "Disposition of Occurrents with respect to their capacity to suggest, solicit or enable participation, reaction, or continuation."@en ,
                       "Disposizione degli Occorrenti rispetto alla loro capacità di suggerire, sollecitare o abilitare partecipazione, reazione o continuazione."@it ;
           :modality "possibility" .


###  https://isagog.com/ontology/top#EntityDescriptor
:EntityDescriptor rdf:type owl:Class ;
                  rdfs:subClassOf :Description ;
                  rdfs:label "Entity Descriptor"@en ,
                            "Descrittore di Entità"@it ;
                  rdfs:comment "Descriptions referring to entities"@en ,
                              "Descrizioni che si riferiscono a entità"@it .


###  https://isagog.com/ontology/top#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :Occurrent ;
       rdfs:label "Event"@en ,
                 "Evento"@it ;
       rdfs:comment "A temporally extended occurrence composed of heterogeneous parts or phases."@en ,
                   "Un'occorrenza temporalmente estesa composta da parti o fasi eterogenee."@it .


###  https://isagog.com/ontology/top#Information
:Information rdf:type owl:Class ;
             rdfs:subClassOf :Intangible ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :yields ;
                               owl:someValuesFrom :Sign
                             ] ;
             rdfs:label "Information"@en ,
                       "Informazione"@it ;
             rdfs:comment "Intangible objects that yields signs, which may be instantiated in many physical forms, e.g. documents, descriptors, classifiers such as tags or topics, word types (lexemes) and tokens."@en ,
                         "Oggetti intangibili che producono segni, che possono essere istanziati in molte forme fisiche, es. documenti, descrittori, classificatori come tag o argomenti, tipi di parole (lessemi) e token."@it ;
             :dolce "http://www.loa-cnr.it/ontologies/DUL.owl#InformationEntity"@en .


###  https://isagog.com/ontology/top#Intangible
:Intangible rdf:type owl:Class ;
            owl:disjointWith :Occurrent ;
            rdfs:label "Intangible"@en ,
                      "Intangibile"@it ;
            rdfs:comment "Immaterial thing (without direct temporal-spatial attributes), which is socially recognized, such as: informative objects (e.g. documents), ideas, theories, qualities"@en ,
                        "Cosa immateriale (senza attributi spazio-temporali diretti), che è socialmente riconosciuta, come: oggetti informativi (es. documenti), idee, teorie, qualità"@it .


###  https://isagog.com/ontology/top#Material
:Material rdf:type owl:Class ;
          rdfs:subClassOf :Continuant ;
          rdfs:label "Material"@en ,
                    "Materiale"@it ;
          rdfs:comment "A non-sortal continuant that exists as a mass or quantity without inherent boundaries or a distinct identity, persisting through time while undergoing potential changes in form or composition. Material is typically characterized by its divisibility and its capacity to constitute or combine with other entities to form objects or structures."@en ,
                      "Un continuante non-sortale che esiste come massa o quantità senza confini intrinseci o un'identità distinta, persistendo nel tempo mentre subisce potenziali cambiamenti di forma o composizione. Il materiale è tipicamente caratterizzato dalla sua divisibilità e dalla sua capacità di costituire o combinarsi con altre entità per formare oggetti o strutture."@it .


###  https://isagog.com/ontology/top#Mention
:Mention rdf:type owl:Class ;
         rdfs:subClassOf :Sentence ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :referred_by ;
                           owl:someValuesFrom :EntityDescriptor
                         ] ;
         rdfs:label "Mention"@en ,
                   "Menzione"@it ;
         rdfs:comment "A textual or verbal occurrence that denotes an entity (e.g., a name). Mentions may sometimes be erroneous, either by referring to a non-existent entity or by misidentifying the intended referent."@en ,
                     "Un'occorrenza testuale o verbale che denota un'entità (es., un nome). Le menzioni possono talvolta essere erronee, sia riferendosi a un'entità inesistente sia identificando erroneamente il referente inteso."@it .


###  https://isagog.com/ontology/top#Object
:Object rdf:type owl:Class ;
        rdfs:subClassOf :Sortal ;
        rdfs:label "Object"@en ,
                  "Oggetto"@it ;
        rdfs:comment "Non-agentive sortal"@en ,
                    "Sortale non agentivo"@it .


###  https://isagog.com/ontology/top#Occurrent
:Occurrent rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :occurs_in ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Place
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :time_coordinate ;
                             owl:someValuesFrom xsd:string
                           ] ;
           rdfs:label "Occurrent"@en ,
                     "Occorrente"@it ;
           rdfs:comment "Thing that unfold or occur over time, such as events, processes, or activities. These entities are not wholly present at any single moment but are extended in time, contrasting with 'Continuants', which persist while maintaining identity."@en ,
                       "Cosa che si dispiega o occorre nel tempo, come eventi, processi o attività. Queste entità non sono completamente presenti in un singolo momento ma sono estese nel tempo, in contrasto con i 'Continuanti', che persistono mantenendo l'identità."@it ;
           :dolce "http://www.loa-cnr.it/ontologies/DUL.owl#Event"@en .


###  https://isagog.com/ontology/top#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf :SocialGroup ;
              rdfs:label "Organization"@en ,
                        "Organizzazione"@it ;
              rdfs:comment "Social group of people working together towards common goals, governed by defined rules and processes"@en ,
                          "Gruppo sociale di persone che lavorano insieme verso obiettivi comuni, governato da regole e processi definiti"@it .


###  https://isagog.com/ontology/top#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf :Agent ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :anthroponym ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ;
        rdfs:label "Person"@en ,
                  "Persona"@it ;
        rdfs:comment "Rational intentional agent - human being"@en ,
                    "Agente razionale intenzionale - essere umano"@it ;
        :schema "https://schema.org/Person" .


###  https://isagog.com/ontology/top#Place
:Place rdf:type owl:Class ;
       rdfs:subClassOf :Continuant ;
       rdfs:label "Place"@en ,
                 "Luogo"@it ;
       rdfs:comment "Identified portion of space"@en ,
                   "Porzione identificata di spazio"@it .


###  https://isagog.com/ontology/top#Quality
:Quality rdf:type owl:Class ;
         rdfs:subClassOf :Intangible ;
         rdfs:label "Quality"@en ,
                   "Qualità"@it ;
         rdfs:comment "Aspect of a spatio-temporal entity, as perceived or conceptually elaborated. Differently from Concepts, Qualities are not socially constructed."@en ,
                     "Aspetto di un'entità spazio-temporale, come percepito o elaborato concettualmente. Diversamente dai Concetti, le Qualità non sono socialmente costruite."@it ;
         :dolce "http://www.loa-cnr.it/ontologies/DUL.owl#Quality"@en .


###  https://isagog.com/ontology/top#Relationship
:Relationship rdf:type owl:Class ;
              rdfs:subClassOf :Statement ;
              rdfs:label "Relationship"@en ,
                        "Relazione"@it ;
              rdfs:comment "A formal statement (assertion) that specifies how two entities are connected or associated within a given context, describing the nature, direction, and type of interaction or dependency between them."@en ,
                          "Un enunciato formale (asserzione) che specifica come due entità sono connesse o associate all'interno di un dato contesto, descrivendo la natura, direzione e tipo di interazione o dipendenza tra loro."@it .


###  https://isagog.com/ontology/top#Sentence
:Sentence rdf:type owl:Class ;
          rdfs:subClassOf :Sign ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :has_logical_part ;
                            owl:someValuesFrom :Word
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :expression ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ;
          rdfs:label "Sentence"@en ,
                    "Frase"@it ;
          rdfs:comment "Syntactic linguistic occurrence (token) regardless of realization (written or spoken)"@en ,
                      "Occorrenza linguistica sintattica (token) indipendentemente dalla realizzazione (scritta o parlata)"@it .


###  https://isagog.com/ontology/top#Sign
:Sign rdf:type owl:Class ;
      rdfs:subClassOf :Intangible ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :interpretant ;
                        owl:someValuesFrom :Concept
                      ] ,
