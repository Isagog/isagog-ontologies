:referred_by rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :has_inherent ;
             rdfs:comment "Being referred by a sign"@en ,
                          "Essere riferito da un segno"@it ;
             rdfs:label "referred by"@en ,
                        "riferito da"@it .


###  https://isagog.com/ontology/top#relatedness
:relatedness rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:comment "generic relatedness"@en ,
                          "relazione generica"@it ;
             rdfs:label "relatedness"@en ,
                        "relazione"@it .


