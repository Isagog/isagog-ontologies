###  https://isagog.com/ontology/top#hosts
:hosts rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :collocation ;
       owl:inverseOf :occurs_in ;
       rdfs:domain :Place ;
       rdfs:range :Occurrent ;
       rdfs:comment "Hosting relation from place to event"@en ,
                    "Relazione di ospitalità dal luogo all'evento"@it ;
       rdfs:label "hosts"@en ,
                  "ospita"@it .


###  https://isagog.com/ontology/top#inherence
:inherence rdf:type owl:ObjectProperty ;
           rdfs:comment "Relation of information objects, their subjects and substrate"@en ,
                        "Relazione degli oggetti informativi, i loro soggetti e substrato"@it ;
           rdfs:label "inerenza"@it ,
                      "inherence"@en .


###  https://isagog.com/ontology/top#inheres_in
:inheres_in rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :inherence ;
            rdfs:domain :Intangible ;
            rdfs:comment "Any relation involved in semiotic processes"@en ,
                         "Qualsiasi relazione coinvolta in processi semiotici"@it ;
            rdfs:label "inerisce in"@it ,
                       "inheres in"@en .


###  https://isagog.com/ontology/top#initiated_by
:initiated_by rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :has_participant ;
              owl:inverseOf :initiates ;
              rdfs:domain :Occurrent ;
              rdfs:comment "Agentive participation"@en ,
                           "Partecipazione agentiva"@it ;
              rdfs:label "initiated by"@en ,
                         "iniziato da"@it .


###  https://isagog.com/ontology/top#initiates
:initiates rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :participate_in ;
           rdfs:comment "Agentive initiation of events"@en ,
                        "Iniziazione agentiva di eventi"@it ;
           rdfs:label "initiates"@en ,
                      "inizia"@it .


###  https://isagog.com/ontology/top#interpretant
:interpretant rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :relatedness ;
              rdfs:domain :Sign ;
              rdfs:range :Concept ;
              rdfs:comment "collega un Segno a un Concetto come suo Interpretante, cioè l'effetto o comprensione che il segno produce in una mente, secondo la triade semiotica di Peirce."@it ,
                           "links a Sign to a Concept as its Interpretant, i.e., the effect or understanding the sign produces in a mind, according to Peirce's semiotic triad."@en ;
              rdfs:label "interpretant"@en ,
                         "interpretante"@it .


###  https://isagog.com/ontology/top#is_location_of
:is_location_of rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :collocation ;
                owl:inverseOf :located_in ;
                rdfs:domain :Place ;
                rdfs:range :Continuant ;
                rdfs:comment "Location relation from place to entity"@en ,
                             "Relazione di ubicazione dal luogo all'entità"@it ;
                rdfs:label "is location of"@en ,
